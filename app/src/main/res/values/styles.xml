<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="OutlinedCard" parent="Widget.MaterialComponents.CardView">
        <item name="cardCornerRadius">12dp</item>
        <item name="cardElevation">4dp</item>
        <item name="strokeColor">?attr/colorOutline</item>
        <item name="strokeWidth">1dp</item>
    </style>
    <style name="CalendarDateText" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>
    <style name="CalendarWeekDayText" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="BottomNavActiveIndicator" parent="">
        <item name="android:background">@drawable/bottom_nav_active_bg</item>
    </style>

</resources>
